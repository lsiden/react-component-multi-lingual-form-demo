{
  "name": "react-seed-component",
  "version": "1.0.0",
  "description": "Reeact seed component for development.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lsiden/local-number-input.git"
  },
  "keywords": [
    "local",
    "numeric",
    "edit",
    "component"
  ],
  "author": "Lawrence Siden",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/lsiden/local-number-input/issues"
  },
  "homepage": "https://github.com/lsiden/local-number-input#readme",
  "entry": "dist/bundle.js",
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "sprintf-js": "^1.1.1",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "redux-tools": "^1.0.3",
    "regenerator-runtime": "^0.10.5",
    "style-loader": "^0.18.2",
    "uglify-js": "^3.0.27",
    "webpack-dev-server": "^2.7.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "test": "NODE_ENV=test jest --verbose",
    "test:watch": "yarn test -- --watch",
    "start": "NODE_ENV=dev webpack-dev-server --config=./webpack.demo.js",
    "build": "NODE_ENV=prod webpack",
    "build:demo": "NODE_ENV=prod webpack --config=./webpack.demo.js"
  }
}
